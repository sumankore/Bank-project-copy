@model IEnumerable<Bank_project.Models.Registration>

@{
    ViewBag.Title = "Account list";
}

<h2>Account list</h2>

<p>
    @Html.ActionLink("Create New", "accreate")
</p>
<table class="table">
    <tr>
        <th>
            @*@Html.DisplayNameFor(model => model.accountid)*@
            Userid
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.accountid)*@
            Account Number
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.Name)*@
            Name
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DOB)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Gender)
        </th>
        <th>
            @*@html.displaynamefor(model => model.acctype)*@
            Account Type
        </th>
        <th>
            @*@html.displaynamefor(model => model.acctype)*@
            Mobile
        </th>
        <th>
            @*@html.displaynamefor(model => model.acctype)*@
            AAdhar number
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.isActive)*@
            IsActive
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.userid)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.accountnumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DOB)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Gender)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.acctypename)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.mobile)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.aadhar)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.isActive)
            </td>
            <td>
                @Html.ActionLink("Edit", "Editcustacc", new { id = item.userid }) |
                @Html.ActionLink("Details", "Details", new { id = item.userid }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.userid }) |
                @Html.ActionLink("Transact", "customerbankdetails", new { id = item.accountnumber })

            </td>
        </tr>
    }

</table>
