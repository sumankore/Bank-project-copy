@model  Bank_project.Models.resultviewmodel

@{
    ViewBag.Title = "Transactions";
}

<h2>Transactions</h2>



Account Number:@ViewBag.acnum||
              Account Type: @ViewBag.accountype||
            @*Bank Balance :@ViewBag.bankbal*@
            <table class="table">
                <tr>
                    <th>
                        @*@Html.DisplayNameFor(model => model.TransactionID)*@Transaction ID
                    </th>
                    <th>
                        @*@Html.DisplayNameFor(model => model.Transaction_Date)*@ Transaction Date
                    </th>
                    <th>
                        @*@Html.DisplayNameFor(model => model.previous_balance)*@ Previous Balance
                    </th>
                    <th>
                        @*@Html.DisplayNameFor(model => model.Transactionamount)*@ Transaction Amount
                    </th>

                    @*<th>
                            @Html.DisplayNameFor(model => model.acctypename)
                        </th>*@
                    <th>
                        @*@Html.DisplayNameFor(model => model.tr_type)*@ Transaction Type
                    </th>
                    <th>
                        @*@Html.DisplayNameFor(model => model.bank_balance)*@ Bank Balance
                    </th>
                </tr>

                @foreach (var item in Model.Transactionsviewlist)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.TransactionID)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Transaction_Date)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.previous_balance)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Transactionamount)
                        </td>

                        @*<td>
                                @Html.DisplayFor(modelItem => item.acctypename)
                            </td>*@
                        <td>
                            @Html.DisplayFor(modelItem => item.tr_type)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.bank_balance)
                        </td>



                        @*<td>
                                @Html.ActionLink("Edit", "Edit", new { id = item.id }) |
                                @Html.ActionLink("Details", "Details", new { id = item.id }) |
                                @Html.ActionLink("Delete", "Delete", new { id = item.id })
                            </td>*@
                    </tr>
                }
                <p>

                </p>

            </table>

            @Html.ActionLink("Make a new transaction", "transfer", new { id = @ViewBag.acnum })
            @Html.ActionLink("Download statement", "PrintPartialViewToPdf", new { id = @ViewBag.acnum })
